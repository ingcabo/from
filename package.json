{
  "name": "media_server_admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@loadable/component": "^5.7.0",
    "@svgr/webpack": "4.1.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-named-asset-import": "^0.3.1",
    "bootstrap": "^4.3.1",
    "chalk": "2.4.2",
    "dotenv": "7.0.0",
    "dotenv-expand": "5.0.0",
    "fs-extra": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.4",
    "react-dom": "^16.5.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-sortablejs": "^1.5.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "resolve": "1.10.0",
    "searchkit": "^2.3.1-alpha.8",
    "sortablejs": "1.6.1",
    "universal-cookie": "^3.0.4"
  },
  "scripts": {
    "build:prod1": "webpack -p --progress --env.type prod --env.mode production",
    "build:qa": "webpack  -p --progress --env.type qa --env.mode production",
    "build:stage1": "webpack -p --progress --env.type stage --env.mode production",
    "build:dev": "webpack -p --progress --env.type dev --env.mode production",
    "start": "webpack-dev-server  --hot --inline --compress --progress --colors --env.type dev --env.mode development"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.5.0",
    "babel-preset-react-app": "^7.0.2",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chokidar": "^2.1.5",
    "clean-webpack-plugin": "^2.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "2.1.1",
    "file-loader": "3.0.1",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "24.5.0",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "1.4.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.1.0"
  }
}
